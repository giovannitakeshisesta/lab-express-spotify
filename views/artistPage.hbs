<div class="page">
{{#each artistsData}}
    <div class="card" style="width: 25rem; ">
        
        <img src="{{images.[1].url}}" class="card-img-top" alt="{{name}}"> 

        <h5 class="card-title">{{name}}</h5>

        <div class="card-footer">
            <a href="/albums/{{id}}" class="btn btn-primary">View Albums</a>
        </div>

    </div>
{{/each}} 
</div> 